<% provide(:title, "Archived Contacts")%>

<div class="row">
  <div class="col-md-8 col-md-offset-2">

    <h1 class="text-center">Archived Contacts</h1>

    <ul class="list-group">
      <% if @archived_contacts.present? %>
        <%= render partial: "contact", collection: @archived_contacts, as: :contact %>
      <% else %>
        <%= "No archived contacts." %>
      <% end %>
    </ul>

  </div>
</div>
